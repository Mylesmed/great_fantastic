def clear():
    os.system('clear')

def fprint(string):
    for letter in string:
        sys.stdout.write(letter)
        sys.stdout.flush()
        time.sleep(scrollspeed)
    print()

scrollspeed = .03

fprint("To begin, enter your first name.")
time.sleep(.2)
name = input("--->   ")
