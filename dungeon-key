time.sleep(2)
fprint("")
fprint("")
fprint("Would you like to tell us about your experience and submit your answers for behind-the-scenes analytics? (yes/no)")
reviewing = input("--->  ")
if reviewing == "yes":
    fprint("Terminating simulation...")
    time.sleep(1)
    fprint("Please wait while we load your user review...")
    time.sleep(2)
else:
    fprint("The Brown Overcoat Association understands you're a busy person with places to be.")
    time.sleep(1.78)
    fprint("Terminating session...")
    time.sleep(2)
    exit()

while reviewing == "yes":
    clear()
    fprint("The Brown Overcoat Association thanks you for your time in the A.S.P.!") 
    fprint("Are you satisfied with your experience in this simulation? Enter 'yes' or 'no' to continue.")
    review = input("--->  ")
    if review != "yes" and review != "no":
        clear()
        fprint("Error. Ask question again?")
        time.sleep(1)
        review2 = input("--->  ")
        if review2 != "yes" and review2 != "no":
            clear()
            fprint("Ok. Terminating session...")
            exit()
        elif review2 == "yes":
            clear()
            continue
        elif review2 == "no":
            clear()
            fprint("Ok. Thank you for playing. See you later.")
            fprint("Terminating session...")
            time.sleep(2)
            break
    elif review == "yes" or review == "no":
        clear()
        fprint("Your feedback is appreciated. If you feel the need to elaborate, please do so. Otherwise, respond with 'No thanks.'")
        why = input("--->  ")
        clear()
        fprint("We appreciate your review. The B.O.A. thanks you for your time.")
        fprint("")
        fprint("")
        time.sleep(2)
        break






gmail_user = 'ephesiyies@gmail.com'
gmail_password = 'EphesiyiesWizard1124'

sent_from = gmail_user
to = ['medynskimyles@gmail.com']
subject = 'CYOA user review'
body = "User:  " + name
bodywizname = "Wizard Name:" + wizname
body2 = "Enjoyed:  " + review
body3 = "Review:   " + why

selections = "Choices made: " + str(choice1) + choice2a + choice2b + choice3a + choice3b + choice3c + choice3d + choice4a + choice4b + choice4c + choice4d + choice4e + choice4f + choice4g + choice4h + choice5a + choice5b + choice5c + choice5d + choice5e + choice5f + choice5g + choice5h + choice5i + choice5j + choice5k + choice5l + choice5m + choice5n + choice5o + choice5p

email_text = """\
From: %s
To: %s
Subject: %s

%s
%s
%s
%s

%s
""" % (sent_from, ", ".join(to), subject, body, bodywizname, body2, body3, selections)

try:
    smtp_server = smtplib.SMTP_SSL('smtp.gmail.com', 465)
    smtp_server.ehlo()
    smtp_server.login(gmail_user, gmail_password)
    smtp_server.sendmail(sent_from, to, email_text)
    smtp_server.close()
    fprint ("Have a nice day.")
    fprint("")
except Exception as ex:
    fprint ("Something went wrong....", ex)






